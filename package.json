    "scripts": {
        "clean": "rimraf ./src/dist",
        "webpack-build-watch": "webpack --progress --watch",
        "webpack-server": "webpack-dev-server --open --inline --progress --port 3000 --content-base src",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "start:nodemon": "tsc && concurrently \"tsc -w\" \"nodemon server.js\" ",
        "start": "tsc && concurrently \"tsc -w\" \"node server.js\" "
    },
